<!DOCTYPE html>
<html>
<head>
  <meta charset="UTF-8">
  <title>{% block title %}Welcome!{% endblock %}</title>
  {% block stylesheets %}
  {{ encore_entry_link_tags('app') }}
  {% endblock %}
</head>
<body>
  <div class="login-reg-panel">
      {% block menu %}
          {% include('menu.html.twig') %}
      {% endblock %}

      <div class="white-panel">
        <div class="login-show">
            {% block body %}{% endblock %}
        </div>
    </div>

  </div>
  {% block javascripts %}
  {{ encore_entry_script_tags('app') }}
{% endblock %}
</body>
</html>
